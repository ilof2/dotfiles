cmd + ctrl + alt - j : yabai -m window --focus south
cmd + ctrl + alt - k : yabai -m window --focus north
cmd + ctrl + alt - h : yabai -m window --focus west
cmd + ctrl + alt - l : yabai -m window --focus east

cmd + ctrl + alt - a : yabai -m display --focus west
cmd + ctrl + alt - d : yabai -m display --focus east
cmd + ctrl + alt - s : yabai -m display --focus south
cmd + ctrl + alt - w : yabai -m display --focus north

shift + cmd + ctrl + alt - r : yabai -m space --rotate 270
shift + cmd + ctrl + alt - y : yabai -m space --mirror y-axis
shift + cmd + ctrl + alt - x : yabai -m space --mirror x-axis

shift + alt - t : yabai -m window --toggle float --grid 4:4:1:1:2:2

cmd + ctrl + alt - m : yabai -m window --toggle zoom-fullscreen
cmd + ctrl + alt - e : yabai -m space --balance

shift + alt - j : yabai -m window --swap south
shift + alt - k : yabai -m window --swap north
shift + alt - h : yabai -m window --swap west
shift + alt - l : yabai -m window --swap east

shift + cmd + ctrl + alt - j : yabai -m window --warp south
shift + cmd + ctrl + alt - k : yabai -m window --warp north
shift + cmd + ctrl + alt - h : yabai -m window --warp west
shift + cmd + ctrl + alt - l : yabai -m window --warp east

shift + cmd + ctrl + alt - a: yabai -m window --display west; yabai -m display --focus west;
shift + cmd + ctrl + alt - d: yabai -m window --display east; yabai -m display --focus east;
shift + cmd + ctrl + alt - s: yabai -m window --display south; yabai -m display --focus south;
shift + cmd + ctrl + alt - w: yabai -m window --display north; yabai -m display --focus north;

shift + cmd + ctrl + alt - p: yabai -m window --space prev;
shift + cmd + ctrl + alt - n: yabai -m window --space next;
shift + cmd + ctrl + alt - 1: yabai -m window --space 1;
shift + cmd + ctrl + alt - 2: yabai -m window --space 2;
shift + cmd + ctrl + alt - 3: yabai -m window --space 3;
shift + cmd + ctrl + alt - 4: yabai -m window --space 4;
shift + cmd + ctrl + alt - 5: yabai -m window --space 5;

